{
  "cSpell.words": [
    "iife",
    "tmpl"
  ],
  "files.exclude": {
    ".nyc_output": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.vscode": true,
    "**/*.tmpl": true,
    "node_modules": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    ".nyc_output": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.vscode": true,
    "**/*.tmpl": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/*.tmpl": true
  }
}
