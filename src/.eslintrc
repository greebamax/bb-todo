{
  "extends": "airbnb-base",
  "rules": {
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "defaults"
        ]
      }
    ],
    "import/no-unresolved": [
      "error",
      {
        "ignore": [
          "base"
        ]
      }
    ],
    "import/extensions": "off",
    "no-underscore-dangle": "off",
    "quote-props": [
      "error",
      "consistent-as-needed"
    ]
  },
  "plugins": [
    "import"
  ]
}
