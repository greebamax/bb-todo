{
  "extends": "airbnb-base",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "arrow-body-style": "off",
    "import/no-extraneous-dependencies": "off",
    "spaced-comment": [
      "error",
      "always",
      {
        "line": {
          "markers": ["#region"],
          "exceptions": ["#endregion"]
        }
      }
    ],
    "import/prefer-default-export": "off",
    "import/extensions": ["error", "always", { "ignorePackages": true }]
  },
  "plugins": ["import"]
}
